  pod "Fetcher/RxFetcher"
  pod 'DataMapper'

  target 'ReactantKlozetTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ReactantKlozetUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
